<mapper namespace="com.zhxy.mapper.OrderitemsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zhxy.entity.Orderitems">
        <id column="orderNum" property="orderNum" />
        <id column="orderItem" property="orderItem" />
        <result column="prodId" property="prodId" />
        <result column="quantity" property="quantity" />
